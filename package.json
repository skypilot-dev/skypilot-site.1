{
  "name": "skypilot-site",
  "version": "0.1.14",
  "private": true,
  "description": "SkyPilot website",
  "homepage": "https://github.com/skypilotcc/skypilot-site#readme",
  "bugs": {
    "url": "https://github.com/skypilotcc/skypilot-site/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/skypilotcc/skypilot-site.git"
  },
  "license": "UNLICENSED",
  "author": {
    "name": "Camilla Thorsen",
    "email": "camilla@thorsen.dev"
  },
  "contributors": [
    {
      "name": "William Thorsen",
      "email": "william@thorsen.dev"
    }
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "/lib"
  ],
  "scripts": {
    "cb": "yarn run check-build",
    "cc": "yarn run check-code",
    "ct": "yarn run check-types",
    "build": "echo 'No build step. Skipping.'",
    "check-build": "yarn run check-code && yarn run build",
    "check-code": "yarn run check-types && yarn run lint --quiet && yarn test",
    "check-types": "tsc",
    "compile-ts": "babel ./src --out-dir ./lib --extensions .ts --ignore '**/__tests__/*' --ignore '**/*.d.ts'",
    "dev": "live-server --port=4000 --watch='index.html,public/**' .",
    "lint": "eslint --cache '**/*.{js,ts}'",
    "publish:default": "echo 'No publication step. Skipping.'",
    "test": "jest --config jest.standalone.config.js",
    "test:all": "jest --config jest.config.js",
    "test:int": "jest --config jest.integration.config.js"
  },
  "dependencies": {},
  "devDependencies": {
    "@skypilot/toolchain": "^5.2.3",
    "@types/jquery": "^3.5.6",
    "live-server": "^1.2.1",
    "parcel": "^2.0.0-beta.2",
    "parcel-plugin-clean-dist": "^0.0.6",
    "parcel-plugin-custom-dist-structure": "^1.1.18",
    "parcel-plugin-static-files-copy": "^2.6.0"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "customDistStructure": {
    "config": {
      "public": [
        ".css",
        ".css.map",
        ".jpeg",
        ".jpg",
        ".png",
        ".svg"
      ]
    }
  },
  "staticFiles": {
    "staticPath": "public",
    "excludeGlob": "index.html"
  }
}
